{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "devhealthcareadf24"
		},
		"LS_healthcare_devstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_healthcare_devstorage'"
		},
		"LS_healthcare_sqlDB_config_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_healthcare_sqlDB_config'"
		},
		"LS_load_json_to_bronze notebook_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_load_json_to_bronze notebook'"
		},
		"Loading Data Silver To Gold Table_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Loading Data Silver To Gold Table'"
		},
		"LS_healthcare_api_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://r4.smarthealthit.org/"
		},
		"LS_healthcare_devstorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devhealthcarestorage24.dfs.core.windows.net/"
		},
		"LS_healthcare_sqlDB_config_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "devhalthcaredb2403.database.windows.net"
		},
		"LS_healthcare_sqlDB_config_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "devhealthcaredb"
		},
		"LS_healthcare_sqlDB_config_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Avduser"
		},
		"LS_load_json_to_bronze notebook_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0215-072529-zjj9pyio"
		},
		"Loading Data Silver To Gold Table_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0215-072529-zjj9pyio"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devhealthcareadf24_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_healthcare_devstorage_accountKey": {
						"value": "[parameters('LS_healthcare_devstorage_accountKey')]"
					},
					"LS_healthcare_sqlDB_config_password": {
						"value": "[parameters('LS_healthcare_sqlDB_config_password')]"
					},
					"LS_load_json_to_bronze notebook_accessToken": {
						"value": "[parameters('LS_load_json_to_bronze notebook_accessToken')]"
					},
					"Loading Data Silver To Gold Table_accessToken": {
						"value": "[parameters('Loading Data Silver To Gold Table_accessToken')]"
					},
					"LS_healthcare_api_source_properties_typeProperties_url": {
						"value": "[parameters('LS_healthcare_api_source_properties_typeProperties_url')]"
					},
					"LS_healthcare_devstorage_properties_typeProperties_url": {
						"value": "[parameters('LS_healthcare_devstorage_properties_typeProperties_url')]"
					},
					"LS_healthcare_sqlDB_config_properties_typeProperties_server": {
						"value": "[parameters('LS_healthcare_sqlDB_config_properties_typeProperties_server')]"
					},
					"LS_healthcare_sqlDB_config_properties_typeProperties_database": {
						"value": "[parameters('LS_healthcare_sqlDB_config_properties_typeProperties_database')]"
					},
					"LS_healthcare_sqlDB_config_properties_typeProperties_userName": {
						"value": "[parameters('LS_healthcare_sqlDB_config_properties_typeProperties_userName')]"
					},
					"LS_load_json_to_bronze notebook_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_load_json_to_bronze notebook_properties_typeProperties_existingClusterId')]"
					},
					"Loading Data Silver To Gold Table_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Loading Data Silver To Gold Table_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}